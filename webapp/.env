# Flask基础配置
HOST=0.0.0.0
PORT=5000

# JWT配置
JWT_SECRET_KEY=your_jwt_secret_key_here
# 访问令牌过期时间（秒）
JWT_ACCESS_TOKEN_EXPIRES=5  
# 刷新令牌过期时间（秒）
JWT_REFRESH_TOKEN_EXPIRES=604800  

# 数据库配置

DB_USER=your_mysql_user
DB_PASSWORD=your_mysql_passwd
DB_HOST=your_mysql_ip
DB_NAME=your_mysql_db_name
DB_PORT=your_mysql_port

# 验证码配置
# 验证码过期时间（秒）
CAPTCHA_EXPIRE=300 

# Redis配置
REDIS_HOST=your_redis_ip
REDIS_PORT=your_redis_port
REDIS_DB=your_redis_db
REDIS_PASSWORD=your_redis_passwd

# 队列配置
TASK_QUEUE_PREFIX=task_queue_gpu_
MAX_WORKERS=1

# 状态键配置
GPU_STATUS_KEY_PREFIX=gpu_status:
TASK_PROGRESS_KEY_PREFIX=task_progress:
TASK_RESULT_KEY_PREFIX=task_result:

# 进度通道配置
TASK_PROGRESS_CHANNEL_PREFIX=task_progress_channel:
TASK_COMPLETED_CHANNEL_PREFIX=task_completed:
TASK_FAILED_CHANNEL_PREFIX=task_failed:

# 模型配置
DEFAULT_MODEL_TYPE=default

# 日志配置
LOG_LEVEL=INFO

# 临时文件配置
TEMP_FILE_EXPIRY=24

DASHSCOPE_API_KEY=your_dashscope_api_key